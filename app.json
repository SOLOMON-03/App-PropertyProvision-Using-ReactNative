{
  "expo": {
    "name": "PropertyProvision-App",
    "slug": "PropertyProvision-App",
    "scheme": ["propertyprovision"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Image/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image" : "./assets/Image/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solomon.HomeFinder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Image/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.solomon.HomeFinder"
    },
    "web": {
      "favicon" :"./assets/Image/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa950832-faf6-4ecb-8d82-1f88077b5b75"
      }
    }
  }
}
